package com.forloop;

public class Task5 {
public static void main(String[] args) {
	for (int i = 1; i <= 5; i++) {
		for (int j = 1; j <= i; j++) {
			System.out.print("X ");
					}
		
	System.out.println();

	
	String s = "X";
	
	if (i==2) {
s.replace("X ", "+ ");
	}
System.out.println(i);
	}
	
	
	
	
	
}
}
